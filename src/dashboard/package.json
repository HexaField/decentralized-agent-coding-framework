{
  "name": "mvp-dashboard",
  "private": true,
  "type": "module",
  "dependencies": {
    "express": "^4.19.2",
    "http-proxy": "^1.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/http-proxy": "^1.17.15",
    "@types/node": "^22.5.4",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "scripts": {
    "dev": "tsx server/server.ts",
    "start": "node --enable-source-maps dist/server.js",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --ext .ts server ui",
    "format": "prettier --write \"{server,ui}/**/*.{ts,js,css,html}\"",
    "test": "vitest run"
  }
}