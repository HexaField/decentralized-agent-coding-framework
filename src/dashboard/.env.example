# Shared environment for dashboard server and client

# Server
PORT=8090
DASHBOARD_TOKEN=dashboard-secret
ORCHESTRATOR_URL=http://0.0.0.0:18080
ORCHESTRATOR_TOKEN=orchestrator-secret
# CORS: comma-separated list, or * for open (avoid * if using credentials)
CORS_ORIGINS=https://localhost:5173

# UI / Vite (client)
# The server base the client uses for API/SSE/embed
VITE_SERVER_URL=https://localhost:8090
# Optionally expose Vite dev URL to server proxy (rarely needed)
VITE_DEV_URL=https://localhost:5173
VITE_DASHBOARD_TOKEN=dashboard-secret
